<div class="master round">
<table>
<tr>
<td>
<%= button_to_function "Play", "playAll();", :class => "masterButton" %>
</td><td>
<%= button_to_function "Pause", "pauseAll();", :class => "masterButton" %>
</td><td>
<%= button_to_function "Stop", "stopAll();", :class => "masterButton" %>
</td><td>
<%= button_to_function "RW", "rw();", :class => "masterButton" %>
</td><td>
<%= button_to_function "FF", "ff();", :class => "masterButton" %>
</td>
</tr>
</table>
<strong>Volume</strong>
<input type="range" min="0.0" max="1.0" value="0.0" step="0.01" onchange="volumeAll(this.value);" />


</div>