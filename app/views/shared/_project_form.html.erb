<div class="projectFormContainer">
<div class="contentPad">
<%= form_for ([current_user, @project]), :html => {:multipart => true} do |f| %>
  <%= render 'shared/error_messages', :object => f.object %>
 <div class="field">Project Name:  
    <%= f.text_area :name, :size=>"20" %>
  </div>
   <div class="field">Project Description:    
    <%= f.text_area :description, :size=>"20" %>
  </div>
  <div class="actions">
    <%= f.submit "Create Project" %>
  </div>
<% end %>
</div>
</div>